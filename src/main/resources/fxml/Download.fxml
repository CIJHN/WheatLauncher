<?import com.jfoenix.controls.JFXTableView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<StackPane xmlns="http://javafx.com/javafx/2.2">
    <HBox>
        <Label text="%download.id"/>
        <Label id="downloadID"/>
    </HBox>
    <JFXTableView columnsDraggable="false" fixedSize="true">
        <columns>
            <TableColumn id="downloadURL" text="%download.url"/>
            <TableColumn id="downloadProgress" text="%download.progress"/>
        </columns>
    </JFXTableView>
</StackPane>
